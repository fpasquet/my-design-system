{
  "name": "my-design-system",
  "version": "0.0.1",
  "type": "module",
  "main": "./dist/my-design-system.umd.cjs",
  "module": "./dist/my-design-system.js",
  "source": "src/index.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/my-design-system.js",
      "require": "./dist/my-design-system.umd.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "postinstall": "yarn build:design-tokens",
    "build:design-tokens": "style-dictionary build --config sd.config.cjs",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "stylelint": "stylelint 'src/**/*.scss'",
    "test": "vitest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "prettier": "@eleven-labs/prettier-config",
  "dependencies": {
    "@klass/core": "^3.4.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-polymorphed": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "resolutions": {
    "jackspeak": "2.1.1"
  },
  "devDependencies": {
    "@eleven-labs/eslint-config": "^1.0.0",
    "@eleven-labs/prettier-config": "^1.0.0",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/addon-interactions": "^7.5.1",
    "@storybook/addon-links": "^7.5.1",
    "@storybook/blocks": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-vite": "^7.5.1",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^16.18.59",
    "@types/react": "^18.2.30",
    "@types/react-dom": "^18.2.14",
    "@vitejs/plugin-react": "^4.1.0",
    "@vitest/coverage-c8": "^0.33.0",
    "autoprefixer": "^10.4.16",
    "classnames": "^2.3.2",
    "eslint": "^8.52.0",
    "jsdom": "^22.1.0",
    "node-sass": "^9.0.0",
    "postcss": "^8.4.31",
    "postcss-normalize": "^10.0.1",
    "postcss-scss": "^4.0.9",
    "prettier": "^3.0.3",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "sass": "^1.69.4",
    "storybook": "^7.5.1",
    "style-dictionary": "^3.8.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "stylelint-prettier": "^4.0.2",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-dts": "^3.6.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6"
  }
}
